{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "lib": ["es2015"],
    "typeRoots": ["./env.d.ts", "node_modules/@types"],
    "outDir": "dist",
    "noEmit": false,
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "noUnusedLocals": false,
    "skipLibCheck": true
  }
}
